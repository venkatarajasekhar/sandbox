# Common configuration
defaults 
    # Fetch emails every 10 min
	set daemon 600
	set logfile .fetchmail.log

poll in-pop-master proto POP3
	user 'emmanuel_mayssat@lynceantech.com' pass "XXXX" is 'emayssat' here
	#mda "/usr/bin/procmail -m /home/nfs/emayssat/.procmailrc"
	#mda "/usr/bin/procmail"
	mda "procmail -f %F"
    no keep
    no rewrite
