/usr/bin/mysql -p <<EOT
#-------------------
SHOW DATABASES;
#-------------------
SHOW TABLES FROM mysql;
#-------------------
SHOW TABLES FROM documents;
#-------------------
SHOW TABLES FROM parts;
#-------------------
SHOW TABLES FROM test;
#-------------------
EOT

/usr/bin/mysqlshow -p documents;
